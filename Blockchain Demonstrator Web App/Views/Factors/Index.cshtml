@model IEnumerable<BlockchainDemonstratorApi.Models.Classes.Factors>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.retailTransport)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.manuTransport)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.procTransport)
