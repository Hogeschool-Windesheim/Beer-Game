            <th>
                @Html.DisplayNameFor(model => model.farmerProductPrice)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.harvesterProductPrice)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.setupCost)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.initialCapital)
            </th>
            <th></th>
        </tr>
