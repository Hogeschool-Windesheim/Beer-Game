            </td>
            <td>
                @Html.DisplayFor(modelItem => item.manuProductPrice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.procProductPrice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.farmerProductPrice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.harvesterProductPrice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.setupCost)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.initialCapital)
            </td>
